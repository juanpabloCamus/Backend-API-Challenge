{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style": ["error", "windows"],
        "semi":"warn",
        "consistent-return": "off",
        "no-unused-vars":"off",
        "no-underscore-dangle":"off"
    }
}
